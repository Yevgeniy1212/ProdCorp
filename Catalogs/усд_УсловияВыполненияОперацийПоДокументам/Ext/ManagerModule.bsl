#Если Сервер Или ТолстыйКлиентОбычноеПриложение Или ВнешнееСоединение Тогда
	
	Функция ПолучитьОписаниеУсловия(ВыбранныеДанные) Экспорт
		
		Текст = "";
		
		мВидСравнения = ВыбранныеДанные.ВидСравнения;
		ЭтоСписок = Ложь;
		Если мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВСписке 
				ИЛИ мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВСпискеПоИерархии
				ИЛИ мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.НеВСписке
				ИЛИ мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.НеВСпискеПоИерархии Тогда
			ЭтоСписок = Истина;
		КонецЕсли;
		//Если Найти(ВыбранныеДанные.ИмяРеквизита,"АвтовыборПоТипу:")<>0 Тогда
		//	Если ВыбранныеДанные.УсловиеНаВложенноеПоле Тогда
		//		Текст = Текст + "Вложенное поле """+ВыбранныеДанные.ВложенноеПоле+""" значения субконто типа """+Строка(ТипЗнч(ВыбранныеДанные.Типизатор))+""" - " + Символы.ПС;
		//	Иначе
		//		Текст = Текст + "Значение субконто типа """+Строка(ТипЗнч(ВыбранныеДанные.Типизатор))+""" - " + Символы.ПС;
		//	КонецЕсли;
		//КонецЕсли;
		Текст = Текст + "Поле: " + ВыбранныеДанные.ПредставлениеРеквизита + Символы.ПС + " (программное имя: "+ВыбранныеДанные.ИмяРеквизита+") " + Символы.ПС + ВРег(ВыбранныеДанные.ВидСравнения)+" ";
		Если ВыбранныеДанные.РежимСравнения = Перечисления.усд_РежимыСравненияДляУсловий.СравнениеСПроизвольнымЗначением Тогда
			Если ЭтоСписок Тогда
				//Если ЗначениеЗаполнено(ВыбранныеДанные.ПредопределенныйСписок) Тогда
				//	Текст = Текст + Строка(ВыбранныеДанные.ПредопределенныйСписок);
				//Иначе
					СписокЗначений = ?(ВыбранныеДанные.СтрокаСписокСравнения="",Новый СписокЗначений,ЗначениеИзСтрокиВнутр(ВыбранныеДанные.СтрокаСписокСравнения));
					ПредставлениеСписка = "";
					Для Каждого СтрокаСписка Из СписокЗначений Цикл
						ПредставлениеСписка = ПредставлениеСписка + ?(ПредставлениеСписка="","","; ")+Строка(СтрокаСписка.Значение);
					КонецЦикла;
					Текст = Текст +": " + ПредставлениеСписка;
				//КонецЕсли;
			ИначеЕсли мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВИнтервале ИЛИ мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВИнтервалеВключаяГраницы Тогда
				Текст = Текст + ": от " + Строка(ВыбранныеДанные.ЗначениеДляСравнения) + " до "+ Строка(ВыбранныеДанные.ЗначениеДляСравнения2);
			Иначе
				Текст = Текст + Строка(ВыбранныеДанные.ЗначениеДляСравнения);
			КонецЕсли;
		ИначеЕсли ВыбранныеДанные.РежимСравнения = Перечисления.усд_РежимыСравненияДляУсловий.СравнениеСПолемРегистраСведений Тогда
			Попытка
				Регистр = Метаданные.РегистрыСведений[ВыбранныеДанные.ИсточникДанных].Синоним;
			Исключение
				Регистр = ВыбранныеДанные.ИсточникДанных;
			КонецПопытки;
			Если мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВИнтервале ИЛИ мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВИнтервалеВключаяГраницы Тогда
				Текст = Текст + ": от значения поля """ + ВыбранныеДанные.ПолеИсточникаДляСравнения + """ до значения поля """+ ВыбранныеДанные.ПолеИсточникаДляСравнения2+""" регистра сведений """+Регистр+"""";
			Иначе
				Текст = Текст + "значение поля """+ВыбранныеДанные.ПолеИсточникаДляСравнения+""" регистра сведений """+Регистр+"""";
			КонецЕсли;
		ИначеЕсли ВыбранныеДанные.РежимСравнения = Перечисления.усд_РежимыСравненияДляУсловий.СравнениеСПолемПроизвольногоИсточника Тогда
			Если мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВИнтервале ИЛИ мВидСравнения = Перечисления.усд_ВидыСравненияДляУсловий.ВИнтервалеВключаяГраницы Тогда
				Текст = Текст + ": от значения поля """ + ВыбранныеДанные.ПолеИсточникаДляСравнения + """ до значения поля """+ ВыбранныеДанные.ПолеИсточникаДляСравнения2+""" настроенного пользователем запроса";
			Иначе
				Текст = Текст + "значение поля """+ВыбранныеДанные.ПолеИсточникаДляСравнения+""" настроенного пользователем запроса";
			КонецЕсли;
		КонецЕсли;
		
		
		Возврат Текст;
	КонецФункции
	
