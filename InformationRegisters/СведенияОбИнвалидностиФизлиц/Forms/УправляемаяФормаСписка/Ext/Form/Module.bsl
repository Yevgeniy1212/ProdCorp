	Если Параметры.Свойство("ФизЛицоСсылка") ТОгда
		 Список.Отбор.Элементы.Очистить();
	     Поле = Список.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
