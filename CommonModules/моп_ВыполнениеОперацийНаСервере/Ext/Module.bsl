		Движение.Количество 		= Выборка.Количество;
		Движение.Цена 				= ?(Движение.Количество = 0, 0, Движение.СуммаБезНалогов/Движение.Количество);		
		Движение.Валюта 			= Выборка.Валюта;
		Движение.КурсТенге 			= Выборка.КурсВалюты;
		
		Движение.НомерГТД				= Выборка.НомерГТД;
		Движение.ДатаГТД				= Выборка.ДатаГТД;
		Движение.НомерИнвойса			= Выборка.НомерИнвойса;		
		Движение.ДатаИнвойса			= Выборка.ДатаИнвойса;
		Движение.УсловияПоставки		= Выборка.УсловияПоставки;
		Движение.МестоОтгрузки			= Выборка.МестоОтгрузки;
		Движение.ДатаПаспортаСделки		= Выборка.ДатаПаспортаСделки;
		Движение.НомерПаспортаСделки	= Выборка.НомерПаспортаСделки;
		Движение.ДатаОплаты				= Выборка.ДатаОплаты;
		
	КонецЦикла;		
	
	НаборЗаписей.Отбор.Регистратор.Установить(ДанныеДокумента.Ссылка);		
	НаборЗаписей.мПериод          = ДанныеДокумента.Дата;
	НаборЗаписей.мТаблицаДвижений = ТаблицаДвижений;	
	НаборЗаписей.ВыполнитьДвижения();
	НаборЗаписей.Записать();
	
КонецПроцедуры

