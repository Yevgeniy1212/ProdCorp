// Функия возвращает модуль числа
//
Функция ПолучитьМодульЧисла(Число)
	Возврат ?(Число < 0, -Число, Число);	
КонецФункции

// Процедура заполняет данные из дополнительного счета-фактуры, введенного на основании первичного документа
// 
Процедура ЗаполнитьДанныеИзДополнительногоСчетаФактуры(СчетФактура, Запись, Выборка, ДанныеКурсаВалюты, ВалютаРегламентированногоУчета, УчитыватьАкциз = Ложь)
	
	Номенклатура = Выборка.Номенклатура;
	Если Не ЗначениеЗаполнено(Номенклатура) Тогда
		Возврат;
	КонецЕсли;
	СсылкаНадокумент = Выборка.СсылкаНаДокумент;
	ВалютаДокумента = СсылкаНадокумент.ВалютаДокумента;
	КурсВзаиморасчетов = СсылкаНадокумент.КурсВзаиморасчетов;
	КратностьВзаиморасчетов = СсылкаНадокумент.КратностьВзаиморасчетов;
	Если СчетФактура.ВалютаДокумента <> ВалютаДокумента Тогда
		Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Значение реквизита ""Валюта документа"" равное """
		+ СчетФактура.ВалютаДокумента + """ в счете-фактуре, отличается от значения в документе """ + СсылкаНадокумент + """ равное """ + ВалютаДокумента + """", СтатусСообщения.Внимание);		
		Возврат;		
	КонецЕсли;
	Если СчетФактура.КурсВзаиморасчетов <> КурсВзаиморасчетов Тогда
		Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Значение реквизита ""Курс взаиморасчетов"" равное """
		+ СчетФактура.КурсВзаиморасчетов + """ в счете-фактуре, отличается от значения в документе """ + СсылкаНадокумент + """ равное """ + КурсВзаиморасчетов + """", СтатусСообщения.Внимание);		
		Возврат;		
	КонецЕсли;
	Если СчетФактура.КратностьВзаиморасчетов <> КратностьВзаиморасчетов Тогда
		Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Значение реквизита ""Кратность взаиморасчетов"" равное """
		+ СчетФактура.КратностьВзаиморасчетов + """ в счете-фактуре, отличается от значения в документе """ + СсылкаНадокумент + """ равное """ + КратностьВзаиморасчетов + """", СтатусСообщения.Внимание);		
		Возврат;		
	КонецЕсли;
	
	СуммаВключаетНДС = СчетФактура.СуммаВключаетНДС;
	Если УчитыватьАкциз Тогда
	СуммаВключаетАкциз = СчетФактура.СуммаВключаетАкциз;
	КонецЕсли;
	Количество = 0;
	СуммаАкциза = 0;
	
	Если ТипЗнч(Номенклатура) = Тип("СправочникСсылка.Номенклатура") Тогда
		Таблица = СчетФактура.Товары;
		НайденныеСтрокиТовары = Таблица.НайтиСтроки(Новый Структура("Номенклатура, СтавкаНДС", Номенклатура, Выборка.СтавкаНДС));
		
		Таблица = СчетФактура.Услуги;
		НайденныеСтрокиУслуги = Таблица.НайтиСтроки(Новый Структура("Номенклатура, СтавкаНДС", Номенклатура, Выборка.СтавкаНДС));
		
		Если НайденныеСтрокиТовары.Количество() = 0 и НайденныеСтрокиУслуги.Количество() = 0 Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Не найден товар\услуга """
			+ Номенклатура + """:" + Символы.ПС + "Ставка НДС = "
			+ Выборка.СтавкаНДС + "; Единица измерения = " + Выборка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);		
			Возврат;
		КонецЕсли;
		Если НайденныеСтрокиТовары.Количество() > 0 и НайденныеСтрокиУслуги.Количество() > 0 Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
			+ Символы.ПС + "В документе введено несколько товаров\услуг """
			+ Номенклатура + """ с одинаковыми характеристиками:" + Символы.ПС + "Ставка НДС = "
			+ Выборка.СтавкаНДС + "; Единица измерения = " + Выборка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);
			Возврат;			
		КонецЕсли;
		
		ЕдиницаИзмеренияИзКлассификатора = СчетФактура.Метаданные().ТабличныеЧасти.Товары.Реквизиты.ЕдиницаИзмерения.Тип = Новый ОписаниеТипов("СправочникСсылка.КлассификаторЕдиницИзмерения");			
		Количество = Неопределено;
		Сумма = Неопределено;
		СуммаНДС = Неопределено;
		СуммаАкциза = Неопределено;
		Для каждого Строка из НайденныеСтрокиТовары Цикл
			Если Строка.Цена <> Окр(Выборка.Цена, 2) Тогда
				Продолжить;
			КонецЕсли;
			Если ЕдиницаИзмеренияИзКлассификатора Тогда
				Если Строка.ЕдиницаИзмерения <> Выборка.ЕдиницаИзмерения Тогда
					Продолжить;
				КонецЕсли;
			Иначе
				Если Строка.ЕдиницаИзмерения.ЕдиницаПоКлассификатору <> Выборка.ЕдиницаИзмерения Тогда
					Продолжить;
				КонецЕсли;			
			КонецЕсли;
			Если Количество <> Неопределено и Строка.Количество = Количество Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""ТМЗ"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Номенклатура = " + Номенклатура + "; Количество = " + Строка(Строка.Количество) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + "; Единица измерения = " + Строка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(Строка.Количество) = ПолучитьМодульЧисла(Выборка.Количество) Тогда
				Количество = Строка.Количество;				
			КонецЕсли;
			Если СуммаВключаетНДС Тогда
				СуммаБезНалогов = Строка.Сумма - Строка.СуммаНДС;
			Иначе
				СуммаБезНалогов = Строка.Сумма;
			КонецЕсли;
			Если УчитыватьАкциз Тогда
				Если СуммаВключаетАкциз Тогда 
					СуммаБезНалогов = СуммаБезНалогов - Строка.СуммаАкциза;
				КонецЕсли;
			КонецЕсли;
			
			Если Сумма <> Неопределено и СуммаБезНалогов = Сумма Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""ТМЗ"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Номенклатура = " + Номенклатура + "; Сумма = " + Строка(Строка.Сумма) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + "; Единица измерения = " + Строка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(СуммаБезНалогов) = ПолучитьМодульЧисла(Выборка.Сумма) Тогда
				Сумма = СуммаБезНалогов;				
			КонецЕсли;	
			Если СуммаНДС <> Неопределено и Строка.СуммаНДС = СуммаНДС Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""ТМЗ"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Номенклатура = " + Номенклатура + "; Сумма НДС = " + Строка(Строка.СуммаНДС) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + "; Единица измерения = " + Строка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(Строка.СуммаНДС) = ПолучитьМодульЧисла(Выборка.СуммаНДС) Тогда
				СуммаНДС = Строка.СуммаНДС;				
			КонецЕсли;
			Если УчитыватьАкциз Тогда
				Если СуммаАкциза <> Неопределено и Строка.СуммаАкциза = СуммаАкциза Тогда
					Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
					+ Символы.ПС + "В табличной части ""ТМЗ"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
					+ "Номенклатура = " + Номенклатура + "; Сумма НДС = " + Строка(Строка.СуммаНДС) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
					+ Строка.СтавкаНДС + "; Единица измерения = " + Строка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);
					Возврат;
				ИначеЕсли ПолучитьМодульЧисла(Строка.СуммаАкциза) = ПолучитьМодульЧисла(Выборка.СуммаАкциза) Тогда
					СуммаАкциза = Строка.СуммаАкциза;				
				КонецЕсли;	
			КонецЕсли;
		КонецЦикла;
		
		Если Количество = Неопределено или Сумма = Неопределено или СуммаНДС = Неопределено или (УчитыватьАкциз и СуммаАкциза = Неопределено) Тогда
			
		Количество = Неопределено;
		Сумма = Неопределено;
		СуммаНДС = Неопределено;
		Для каждого Строка из НайденныеСтрокиУслуги Цикл
			Если Строка.Цена <> Окр(Выборка.Цена, 2) Тогда
				Продолжить;
			КонецЕсли;						
			Если Количество <> Неопределено и Строка.Количество = Количество Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""ТМЗ"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Номенклатура = " + Номенклатура + "; Количество = " + Строка(Строка.Количество) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(Строка.Количество) = ПолучитьМодульЧисла(Выборка.Количество) Тогда
				Количество = Строка.Количество;								
			КонецЕсли;
			Если СуммаВключаетНДС Тогда
				СуммаБезНалогов = Строка.Сумма - Строка.СуммаНДС;
			Иначе
				СуммаБезНалогов = Строка.Сумма;
			КонецЕсли;	
			Если Сумма <> Неопределено и СуммаБезНалогов = Сумма Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""Услуги"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Номенклатура = " + Номенклатура + "; Сумма = " + Строка(Строка.Сумма) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(СуммаБезНалогов) = ПолучитьМодульЧисла(Выборка.Сумма) Тогда
				Сумма = СуммаБезНалогов;								
			КонецЕсли;	
			Если СуммаНДС <> Неопределено и Строка.СуммаНДС = СуммаНДС Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""Услуги"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Номенклатура = " + Номенклатура + "; Сумма НДС = " + Строка(Строка.СуммаНДС) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(Строка.СуммаНДС) = ПолучитьМодульЧисла(Выборка.СуммаНДС) Тогда
				СуммаНДС = Строка.СуммаНДС;				
			КонецЕсли;	
		КонецЦикла;	
		
		Если Количество = Неопределено или Сумма = Неопределено или СуммаНДС = Неопределено Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Не найдена операция с товаром\услугой """
			+ Номенклатура + """:" + Символы.ПС + "Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
			+ Выборка.СтавкаНДС + "; Единица измерения = " + Выборка.ЕдиницаИзмерения + ";", СтатусСообщения.Внимание);		
			Возврат;			
		КонецЕсли;
	КонецЕсли;
	
	ИначеЕсли ТипЗнч(Номенклатура) = Тип("СправочникСсылка.ОсновныеСредства") Тогда
		
		Таблица = СчетФактура.ОС;
		НайденныеСтроки = Таблица.НайтиСтроки(Новый Структура("ОсновноеСредство, СтавкаНДС",
		Номенклатура,
		Выборка.СтавкаНДС));
		
		Если НайденныеСтроки.Количество() = 0 Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Не найдено основное средство """
			+ Номенклатура + """:" + Символы.ПС + "Ставка НДС = "
			+ Выборка.СтавкаНДС + ";", СтатусСообщения.Внимание);			
			Возврат;
		КонецЕсли;
		
		Сумма = Неопределено;
		СуммаНДС = Неопределено;
		СуммаАкциза = Неопределено;
		Количество = 1;
		Для каждого Строка из НайденныеСтроки Цикл
			Если СуммаВключаетНДС Тогда
				СуммаБезНалогов = Строка.Сумма - Строка.СуммаНДС;
			Иначе
				СуммаБезНалогов = Строка.Сумма;
			КонецЕсли;	
			Если Сумма <> Неопределено и СуммаБезНалогов = Сумма Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""ОС"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Основное средство = " + Номенклатура + "; Сумма = " + Строка(Строка.Сумма) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(СуммаБезНалогов) = ПолучитьМодульЧисла(Выборка.Сумма) Тогда
				Сумма = СуммаБезНалогов;								
			КонецЕсли;	
			Если СуммаНДС <> Неопределено и Строка.СуммаНДС = СуммаНДС Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""ОС"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Основное средство = " + Номенклатура + "; Сумма НДС = " + Строка(Строка.СуммаНДС) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(Строка.СуммаНДС) = ПолучитьМодульЧисла(Выборка.СуммаНДС) Тогда
				СуммаНДС = Строка.СуммаНДС;				
			КонецЕсли;	
		КонецЦикла;
		
		Если Сумма = Неопределено или СуммаНДС = Неопределено Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Не найдена операция с основным средством """
			+ Номенклатура + """:" + Символы.ПС + "Ставка НДС = "
			+ Выборка.СтавкаНДС + ";", СтатусСообщения.Внимание);			
			Возврат;
		КонецЕсли;
		
	Количество = ?(Сумма < 0, -1, 1);		
		
	ИначеЕсли ТипЗнч(Номенклатура) = Тип("СправочникСсылка.НематериальныеАктивы") Тогда
		Таблица = СчетФактура.НМА;
		НайденныеСтроки = Таблица.НайтиСтроки(Новый Структура("НематериальныйАктив, СтавкаНДС",
		Номенклатура,
		Выборка.СтавкаНДС));
		
		Если НайденныеСтроки.Количество() = 0 Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Не найден нематериальный актив """
			+ Номенклатура + """:" + Символы.ПС + "Ставка НДС = "
			+ Выборка.СтавкаНДС + ";", СтатусСообщения.Внимание);			
			Возврат;
		КонецЕсли;
		
		Сумма = Неопределено;
		Для каждого Строка из НайденныеСтроки Цикл
			Если СуммаВключаетНДС Тогда
				СуммаБезНалогов = Строка.Сумма - Строка.СуммаНДС;
			Иначе
				СуммаБезНалогов = Строка.Сумма;
			КонецЕсли;	
			Если Сумма <> Неопределено и СуммаБезНалогов = Сумма Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""НМА"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Нематериальный актив = " + Номенклатура + "; Сумма = " + Строка(Строка.Сумма) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(СуммаБезНалогов) = ПолучитьМодульЧисла(Выборка.Сумма) Тогда
				Сумма = СуммаБезНалогов;								
			КонецЕсли;	
			Если СуммаНДС <> Неопределено и Строка.СуммаНДС = СуммаНДС Тогда
				Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:"
				+ Символы.ПС + "В табличной части ""НМА"" введено несколько операций с одинаковыми характеристиками:""" + Символы.ПС
				+ "Нематериальный актив = " + Номенклатура + "; Сумма НДС = " + Строка(Строка.СуммаНДС) + "; Цена = " + Строка(Выборка.Цена) + "; Ставка НДС = "
				+ Строка.СтавкаНДС + ";", СтатусСообщения.Внимание);
				Возврат;
			ИначеЕсли ПолучитьМодульЧисла(Строка.СуммаНДС) = ПолучитьМодульЧисла(Выборка.СуммаНДС) Тогда
				СуммаНДС = Строка.СуммаНДС;				
			КонецЕсли;
		КонецЦикла;	
		
		Если Сумма = Неопределено или СуммаНДС = Неопределено Тогда
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """:" + Символы.ПС + "Не найдена операция с нематериальным активом """
			+ Номенклатура + """:" + Символы.ПС + "Ставка НДС = "
			+ Выборка.СтавкаНДС + ";", СтатусСообщения.Внимание);			
			Возврат;
		КонецЕсли;		
			Количество = ?(Сумма < 0, -1, 1);		
	Иначе
			Сообщить("Не удалось заполнить данные из дополнительного счета-фактуры """ + СчетФактура + """");
		Возврат;
	КонецЕсли;
		
	Запись.Количество 		= Количество;
	
	Запись.СуммаБезНалогов 	= ОбщегоНазначения.ПересчитатьИзВалютыВВалюту(Сумма, 
	ВалютаДокумента, ВалютаРегламентированногоУчета,
	КурсВзаиморасчетов, ДанныеКурсаВалюты.Курс,
	КратностьВзаиморасчетов, ДанныеКурсаВалюты.Кратность);
	
	Запись.СуммаНДС 	= ОбщегоНазначения.ПересчитатьИзВалютыВВалюту(СуммаНДС, 
	ВалютаДокумента, ВалютаРегламентированногоУчета,
	КурсВзаиморасчетов, ДанныеКурсаВалюты.Курс,
	КратностьВзаиморасчетов, ДанныеКурсаВалюты.Кратность);					
	
	Если УчитыватьАкциз Тогда
		Запись.Акциз 	= ОбщегоНазначения.ПересчитатьИзВалютыВВалюту(СуммаАкциза, 
		ВалютаДокумента, ВалютаРегламентированногоУчета,
		КурсВзаиморасчетов, ДанныеКурсаВалюты.Курс,
		КратностьВзаиморасчетов, ДанныеКурсаВалюты.Кратность);					
	КонецЕсли;

КонецПроцедуры


////////////////////////////////////////////////////////////////////////////////
// ПРОЦЕДУРЫ И ФУНКЦИИ ДЛЯ ОБЕСПЕЧЕНИЯ ПРОВЕДЕНИЯ ДОКУМЕНТА

// Выполняет движения по мониторингу реализации
//
Процедура ВыполнитьДвиженияПоМониторингуРеализации(ДанныеДокумента, НачДата, КонДата, ВалютаРегламентированногоУчета) Экспорт 
		
	Запрос = Новый Запрос();
	
	Запрос.УстановитьПараметр("НачДата", НачДата);
	Запрос.УстановитьПараметр("КонДата", КонДата);
	Запрос.УстановитьПараметр("Организация", ДанныеДокумента.Организация);
	Запрос.УстановитьПараметр("ВалютаРегУчета", ВалютаРегламентированногоУчета);
	
	//Делаем проверку на правильность обращения к перечислению
	//в конфигурации "Бухгалтерии для Казахстана" релиз 1.5.5.4 и ниже, 
	//использовалось перечисление "ВидыОперацийРеализацииТоваровУслуг"
	//начиная с версии 1.5.6.11 используется перечисление "ВидыОперацийРеализацияТоваров"
	Если Метаданные.Перечисления.Найти("ВидыОперацийРеализацияТоваров") <> Неопределено Тогда
		Запрос.УстановитьПараметр("ВидОперацииСтруктурные", Перечисления["ВидыОперацийРеализацияТоваров"].ПередачаСтруктурномуПодразделению);
	ИначеЕсли  Метаданные.Перечисления.Найти("ВидыОперацийРеализацииТоваровУслуг") <> Неопределено Тогда
		Запрос.УстановитьПараметр("ВидОперацииСтруктурные", Перечисления["ВидыОперацийРеализацииТоваровУслуг"].ПередачаСтруктурномуПодразделению);	
	КонецЕсли; 
	
	Запрос.УстановитьПараметр("ВидОперацииСтруктурныеОС", Перечисления.ВидыОперацийПередачиОС.ПередачаСтруктурномуПодразделению);		
	
	Запрос.Текст = "ВЫБРАТЬ
	               |	ВложенныйЗапрос.СчетДоходовБУ,
	               |	ВложенныйЗапрос.Номенклатура,
	               |	ВложенныйЗапрос.Контрагент,
	               |	ВложенныйЗапрос.ДоговорКонтрагента,
	               |	ВложенныйЗапрос.Цена,
	               |	ВложенныйЗапрос.Количество,
	               |	ВложенныйЗапрос.Сумма,
	               |	ВложенныйЗапрос.СтавкаНДС,
	               |	ВложенныйЗапрос.СуммаНДС,
	               |	ВложенныйЗапрос.Дата,
	               |	ВложенныйЗапрос.СсылкаНаДокумент,
	               |	ВложенныйЗапрос.ОснованиеВозврата,
	               |	ВложенныйЗапрос.Организация,
	               |	ВложенныйЗапрос.СуммаАкциза,
	               |	моп_ОбъектыМониторинга.ПинКод,
	               |	моп_ОбъектыМониторинга.КодТНВЭД,
	               |	ВложенныйЗапрос.ЕдиницаИзмерения,
	               |	ВЫБОР
	               |		КОГДА (НЕ ВложенныйЗапрос.Контрагент.Ссылка ЕСТЬ NULL )
	               |				И ВложенныйЗапрос.Контрагент.Ссылка <> ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
	               |				И (ВложенныйЗапрос.Контрагент.ЮрФизЛицо = ЗНАЧЕНИЕ(Перечисление.ЮрФизЛицо.ФизЛицо)
	               |					И (НЕ ВложенныйЗапрос.Контрагент.ИндивидуальныйПредпринимательАдвокатЧастныйНотариус))
	               |			ТОГДА ИСТИНА
	               |		КОГДА ВложенныйЗапрос.Контрагент.Ссылка ЕСТЬ NULL 
	               |				ИЛИ ВложенныйЗапрос.Контрагент.Ссылка = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
	               |			ТОГДА ИСТИНА
	               |		ИНАЧЕ ЛОЖЬ
	               |	КОНЕЦ КАК КонтрагентЯвляетсяЧастнымЛицом
	               |ИЗ
	               |	(ВЫБРАТЬ
	               |		ВозвратТоваровОтПокупателяТовары.СчетДоходовБУ КАК СчетДоходовБУ,
	               |		ВозвратТоваровОтПокупателяТовары.Номенклатура КАК Номенклатура,
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка.Контрагент КАК Контрагент,
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка.ДоговорКонтрагента КАК ДоговорКонтрагента,
	               |		ВозвратТоваровОтПокупателяТовары.Цена КАК Цена,
	               |		ВозвратТоваровОтПокупателяТовары.Количество КАК Количество,
	               |		ВЫБОР
	               |			КОГДА ВозвратТоваровОтПокупателяТовары.Ссылка.СуммаВключаетНДС
	               |					И ВозвратТоваровОтПокупателяТовары.Ссылка.СуммаВключаетАкциз
	               |				ТОГДА ВозвратТоваровОтПокупателяТовары.Сумма - ВозвратТоваровОтПокупателяТовары.СуммаНДС - ВозвратТоваровОтПокупателяТовары.СуммаАкциза
	               |			КОГДА ВозвратТоваровОтПокупателяТовары.Ссылка.СуммаВключаетНДС
	               |				ТОГДА ВозвратТоваровОтПокупателяТовары.Сумма - ВозвратТоваровОтПокупателяТовары.СуммаНДС
	               |			КОГДА ВозвратТоваровОтПокупателяТовары.Ссылка.СуммаВключаетАкциз
	               |				ТОГДА ВозвратТоваровОтПокупателяТовары.Сумма - ВозвратТоваровОтПокупателяТовары.СуммаАкциза
	               |			ИНАЧЕ ВозвратТоваровОтПокупателяТовары.Сумма
	               |		КОНЕЦ КАК Сумма,
	               |		ВозвратТоваровОтПокупателяТовары.СтавкаНДС КАК СтавкаНДС,
	               |		ВозвратТоваровОтПокупателяТовары.СуммаНДС КАК СуммаНДС,
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка.Дата КАК Дата,
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка КАК СсылкаНаДокумент,
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка.ДокументОснование КАК ОснованиеВозврата,
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка.Организация КАК Организация,
	               |		ВозвратТоваровОтПокупателяТовары.СуммаАкциза КАК СуммаАкциза,
	               |		ВозвратТоваровОтПокупателяТовары.ЕдиницаИзмерения КАК ЕдиницаИзмерения
	               |	ИЗ
	               |		Документ.ВозвратТоваровОтПокупателя.Товары КАК ВозвратТоваровОтПокупателяТовары
	               |	ГДЕ
	               |		ВозвратТоваровОтПокупателяТовары.Ссылка.Дата >= &НачДата
	               |		И ВозвратТоваровОтПокупателяТовары.Ссылка.Организация = &Организация
	               |		И ВозвратТоваровОтПокупателяТовары.Ссылка.Дата <= &КонДата
	               |		И ВозвратТоваровОтПокупателяТовары.Ссылка.Проведен = ИСТИНА
	               |		И ВозвратТоваровОтПокупателяТовары.Ссылка.ВалютаДокумента = &ВалютаРегУчета
	               |	
	               |	ОБЪЕДИНИТЬ ВСЕ
	               |	
	               |	ВЫБРАТЬ
	               |		ВозвратТоваровОтПокупателяУслуги.СчетДоходовБУ,
	               |		ВозвратТоваровОтПокупателяУслуги.Номенклатура,
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка.Контрагент,
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка.ДоговорКонтрагента,
	               |		ВозвратТоваровОтПокупателяУслуги.Цена,
	               |		ВозвратТоваровОтПокупателяУслуги.Количество,
	               |		ВЫБОР
	               |			КОГДА ВозвратТоваровОтПокупателяУслуги.Ссылка.СуммаВключаетНДС
	               |				ТОГДА ВозвратТоваровОтПокупателяУслуги.Сумма - ВозвратТоваровОтПокупателяУслуги.СуммаНДС
	               |			ИНАЧЕ ВозвратТоваровОтПокупателяУслуги.Сумма
	               |		КОНЕЦ,
	               |		ВозвратТоваровОтПокупателяУслуги.СтавкаНДС,
	               |		ВозвратТоваровОтПокупателяУслуги.СуммаНДС,
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка.Дата,
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка,
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка.ДокументОснование,
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка.Организация,
	               |		0,
	               |		NULL
	               |	ИЗ
	               |		Документ.ВозвратТоваровОтПокупателя.Услуги КАК ВозвратТоваровОтПокупателяУслуги
	               |	ГДЕ
	               |		ВозвратТоваровОтПокупателяУслуги.Ссылка.Дата >= &НачДата
	               |		И ВозвратТоваровОтПокупателяУслуги.Ссылка.Организация = &Организация
	               |		И ВозвратТоваровОтПокупателяУслуги.Ссылка.Дата <= &КонДата
	               |		И ВозвратТоваровОтПокупателяУслуги.Ссылка.Проведен = ИСТИНА
	               |		И ВозвратТоваровОтПокупателяУслуги.Ссылка.ВалютаДокумента = &ВалютаРегУчета
	               |	
	               |	ОБЪЕДИНИТЬ ВСЕ
	               |	
	               |	ВЫБРАТЬ
	               |		РеализацияТоваровУслугТовары.СчетДоходовБУ,
	               |		РеализацияТоваровУслугТовары.Номенклатура,
	               |		РеализацияТоваровУслугТовары.Ссылка.Контрагент,
	               |		РеализацияТоваровУслугТовары.Ссылка.ДоговорКонтрагента,
	               |		РеализацияТоваровУслугТовары.Цена,
	               |		РеализацияТоваровУслугТовары.Количество,
	               |		ВЫБОР
	               |			КОГДА РеализацияТоваровУслугТовары.Ссылка.СуммаВключаетНДС
	               |					И РеализацияТоваровУслугТовары.Ссылка.СуммаВключаетАкциз
	               |				ТОГДА РеализацияТоваровУслугТовары.Сумма - РеализацияТоваровУслугТовары.СуммаНДС - РеализацияТоваровУслугТовары.СуммаАкциза
	               |			КОГДА РеализацияТоваровУслугТовары.Ссылка.СуммаВключаетНДС
	               |				ТОГДА РеализацияТоваровУслугТовары.Сумма - РеализацияТоваровУслугТовары.СуммаНДС
	               |			КОГДА РеализацияТоваровУслугТовары.Ссылка.СуммаВключаетАкциз
	               |				ТОГДА РеализацияТоваровУслугТовары.Сумма - РеализацияТоваровУслугТовары.СуммаАкциза
	               |			ИНАЧЕ РеализацияТоваровУслугТовары.Сумма
	               |		КОНЕЦ,
	               |		РеализацияТоваровУслугТовары.СтавкаНДС,
	               |		РеализацияТоваровУслугТовары.СуммаНДС,
	               |		РеализацияТоваровУслугТовары.Ссылка.Дата,
	               |		РеализацияТоваровУслугТовары.Ссылка.Ссылка,
	               |		0,
	               |		РеализацияТоваровУслугТовары.Ссылка.Организация,
	               |		РеализацияТоваровУслугТовары.СуммаАкциза,
	               |		РеализацияТоваровУслугТовары.ЕдиницаИзмерения
	               |	ИЗ
	               |		Документ.РеализацияТоваровУслуг.Товары КАК РеализацияТоваровУслугТовары
	               |	ГДЕ
	               |		РеализацияТоваровУслугТовары.Ссылка.Дата >= &НачДата
	               |		И РеализацияТоваровУслугТовары.Ссылка.Дата <= &КонДата
	               |		И РеализацияТоваровУслугТовары.Ссылка.Организация = &Организация
	               |		И РеализацияТоваровУслугТовары.Ссылка.Проведен = ИСТИНА
	               |		И РеализацияТоваровУслугТовары.Ссылка.ВалютаДокумента = &ВалютаРегУчета
	               |		И РеализацияТоваровУслугТовары.Ссылка.ВидОперации <> &ВидОперацииСтруктурные
	               |	
	               |	ОБЪЕДИНИТЬ ВСЕ
	               |	
	               |	ВЫБРАТЬ
	               |		АктОбОказанииПроизводственныхУслугУслуги.СчетДоходовБУ,
