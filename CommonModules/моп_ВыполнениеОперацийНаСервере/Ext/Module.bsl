	ДоступенРеквизитОсновнойСчетФактура = ?(Метаданные.Документы.СчетФактураВыданный.Реквизиты.Найти("ОсновнойСчетФактура") = Неопределено, Ложь, Истина);
	
	ИспользоватьДанныеИзДополнительныхСчетовФактур = моп_Общий.ИспользоватьДанныеИзДополнительныхСчетовФактур(ДанныеДокумента.Дата);
	Если ИспользоватьДанныеИзДополнительныхСчетовФактур = Неопределено Тогда
		ИспользоватьДанныеИзДополнительныхСчетовФактур = Ложь;
	КонецЕсли;
	
	Пока Выборка.Следующий() Цикл
		Док = Выборка.СсылкаНаДокумент;
				
		Движение 					= ТаблицаДвижений.Добавить();
		Движение.Период 			= Выборка.Дата;
		Движение.Счет 				= Выборка.СчетДоходовБУ;
		Движение.Номенклатура 		= Выборка.Номенклатура;
		Движение.ПинКод 		    = Выборка.ПинКод;
		Движение.КодТНВЭД 		    = Выборка.КодТНВЭД;
		Движение.ЕдиницаИзмерения   = Выборка.ЕдиницаИзмерения;
		
		Движение.Контрагент 		= Выборка.Контрагент;	
		Движение.ДоговорКонтрагента = Выборка.ДоговорКонтрагента;
		Движение.СсылкаНаДокумент 	= Док;
		
		Движение.СчетФактура 		= ОбщегоНазначения.НайтиПодчиненныйДокумент(Док, "СчетФактураВыданный");
		
		Если ЗначениеЗаполнено(Движение.СчетФактура) Тогда								
			Движение.СчетФактураЯвляетсяДополнительным = Движение.СчетФактура.Дополнительная;
			Если ДоступенРеквизитОсновнойСчетФактура Тогда				
				Движение.СчетФактураКорректируемый = Движение.СчетФактура.ОсновнойСчетФактура;				
			КонецЕсли;						
		КонецЕсли;	
				
		ДанныеКурсаВалюты = ОбщегоНазначения.ПолучитьКурсВалюты(ВалютаРегламентированногоУчета, Выборка.Дата);		
		
		Если ИспользоватьДанныеИзДополнительныхСчетовФактур и ЗначениеЗаполнено(Движение.СчетФактура) и Движение.СчетФактураЯвляетсяДополнительным Тогда
			ЗаполнитьДанныеИзДополнительногоСчетаФактуры(Движение.СчетФактура, Движение, Выборка, ДанныеКурсаВалюты, ВалютаРегламентированногоУчета, Истина);
		Иначе
				
		Движение.Количество 		= Выборка.Количество;
		
		Движение.СуммаБезНалогов 	= ОбщегоНазначения.ПересчитатьИзВалютыВВалюту(Выборка.Сумма, 
		Док.ВалютаДокумента, ВалютаРегламентированногоУчета,
		Док.КурсВзаиморасчетов, ДанныеКурсаВалюты.Курс,
		Док.КратностьВзаиморасчетов, ДанныеКурсаВалюты.Кратность);		
				
		Движение.Акциз 				= ?(Выборка.СуммаАкциза = NULL, 0 , Выборка.СуммаАкциза);
		
		Движение.СуммаНДС 			= ОбщегоНазначения.ПересчитатьИзВалютыВВалюту(Выборка.СуммаНДС, 
		Док.ВалютаДокумента, ВалютаРегламентированногоУчета,
		Док.КурсВзаиморасчетов, ДанныеКурсаВалюты.Курс,
		Док.КратностьВзаиморасчетов, ДанныеКурсаВалюты.Кратность);				
		
		КонецЕсли;
		
		Движение.СтавкаНДС 			= Выборка.СтавкаНДС;		
		Движение.Цена 				= ?(Движение.Количество = 0, 0, Движение.СуммаБезНалогов/Движение.Количество);
		
		Если Не ЗначениеЗаполнено(Движение.СчетФактураКорректируемый) Тогда
			Если ЗначениеЗаполнено(Выборка.ОснованиеВозврата) Тогда
				Движение.СчетФактураКорректируемый = ОбщегоНазначения.НайтиПодчиненныйДокумент(Выборка.ОснованиеВозврата, "СчетФактураВыданный");
			КонецЕсли;			
		КонецЕсли; 
		
		Движение.Организация 		= Выборка.Организация;
		Движение.КонтрагентЯвляетсяЧастнымЛицом = Выборка.КонтрагентЯвляетсяЧастнымЛицом;
		
	КонецЦикла;		
	
	НаборЗаписей.Отбор.Регистратор.Установить(ДанныеДокумента.Ссылка);		
	НаборЗаписей.мПериод          = ДанныеДокумента.Дата;
	НаборЗаписей.мТаблицаДвижений = ТаблицаДвижений;	
	НаборЗаписей.ВыполнитьДвижения();
	НаборЗаписей.Записать();
		
КонецПроцедуры

// Выполняет движения по мониторингу покупок
// если дополнение встроено в конфигурацию "1С-Рейтинг: Нефтебаза"
//
Процедура ВыполнитьДвиженияПоМониторингуПокупокГСМ(ДанныеДокумента,НачДата, КонДата, ВалютаРегламентированногоУчета) Экспорт
		
	Запрос = Новый Запрос();
	
	Запрос.УстановитьПараметр("НачДата", НачДата);
	Запрос.УстановитьПараметр("КонДата", КонДата);
	Запрос.УстановитьПараметр("Организация", ДанныеДокумента.Организация);
	Запрос.УстановитьПараметр("ВидОперацииИмпорт", Перечисления.ВидыОперацийПоступлениеТоваровУслуг.Импорт);
	Запрос.УстановитьПараметр("ВидОперацииИмпортГСМ", Перечисления.гсм_ВидыОперацийПоступление.Импорт);			
	Запрос.УстановитьПараметр("ВидСубконтоОС", ПланыВидовХарактеристик.ВидыСубконтоТиповые.ОсновныеСредства);
	Запрос.УстановитьПараметр("ВидСубконтоНоменклатура", ПланыВидовХарактеристик.ВидыСубконтоТиповые.Номенклатура);
	Запрос.УстановитьПараметр("ВидСубконтоНМА", ПланыВидовХарактеристик.ВидыСубконтоТиповые.НематериальныеАктивы);	
	Запрос.УстановитьПараметр("ПустойПинКод", Справочники.гсм_ПинКоды.ПустаяСсылка());
	Запрос.УстановитьПараметр("ВидОперацииСтруктурные", Перечисления.ВидыОперацийПоступлениеТоваровУслуг.ОтСтруктурногоПодразделения);
	Запрос.УстановитьПараметр("ВидОперацииСтруктурныеГСМ", Перечисления.гсм_ВидыОперацийПоступление.ОтСтруктурногоПодразделения);
	Запрос.УстановитьПараметр("ВалютаРегУчета", ВалютаРегламентированногоУчета);
	
	Запрос.Текст = "ВЫБРАТЬ
	               |	ВложенныйЗапрос.Ссылка КАК Ссылка,
	               |	МАКСИМУМ(СчетФактураПолученныйДокументыОснования.Ссылка) КАК СчетФактура,
	               |	МАКСИМУМ(ВложенныйЗапрос.Дата) КАК Дата,
	               |	МАКСИМУМ(ВложенныйЗапрос.ВалютаДокумента) КАК ВалютаДокумента,
	               |	МАКСИМУМ(ВложенныйЗапрос.ДоговорКонтрагента) КАК ДоговорКонтрагента,
	               |	МАКСИМУМ(ВложенныйЗапрос.КратностьВзаиморасчетов) КАК КратностьВзаиморасчетов,
	               |	МАКСИМУМ(ВложенныйЗапрос.КурсВзаиморасчетов) КАК КурсВзаиморасчетов,
	               |	МАКСИМУМ(ВложенныйЗапрос.СуммаВключаетНДС) КАК СуммаВключаетНДС,
	               |	МАКСИМУМ(ВложенныйЗапрос.Контрагент) КАК Контрагент,
	               |	МАКСИМУМ(ВложенныйЗапрос.Организация) КАК Организация
	               |ПОМЕСТИТЬ ВТ_ПоступлениеНМА
	               |ИЗ
	               |	(ВЫБРАТЬ
	               |		ПоступлениеНМА.Ссылка КАК Ссылка,
	               |		ПоступлениеНМА.Дата КАК Дата,
	               |		ПоступлениеНМА.ВалютаДокумента КАК ВалютаДокумента,
	               |		ПоступлениеНМА.ДоговорКонтрагента КАК ДоговорКонтрагента,
	               |		ПоступлениеНМА.КратностьВзаиморасчетов КАК КратностьВзаиморасчетов,
	               |		ПоступлениеНМА.КурсВзаиморасчетов КАК КурсВзаиморасчетов,
	               |		ПоступлениеНМА.СуммаВключаетНДС КАК СуммаВключаетНДС,
	               |		ПоступлениеНМА.Контрагент КАК Контрагент,
	               |		ПоступлениеНМА.Организация КАК Организация
	               |	ИЗ
	               |		Документ.ПоступлениеНМА КАК ПоступлениеНМА
	               |	ГДЕ
	               |		ПоступлениеНМА.Дата >= &НачДата
	               |		И ПоступлениеНМА.Дата <= &КонДата
	               |		И ПоступлениеНМА.Организация = &Организация
	               |		И ПоступлениеНМА.Проведен) КАК ВложенныйЗапрос
	               |		ЛЕВОЕ СОЕДИНЕНИЕ Документ.СчетФактураПолученный.ДокументыОснования КАК СчетФактураПолученныйДокументыОснования
	               |		ПО ВложенныйЗапрос.Ссылка = СчетФактураПолученныйДокументыОснования.ДокументОснование
	               |
	               |СГРУППИРОВАТЬ ПО
	               |	ВложенныйЗапрос.Ссылка
	               |
	               |ИНДЕКСИРОВАТЬ ПО
	               |	Ссылка
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	ПоступлениеТоваровУслуг.Ссылка КАК Ссылка,
	               |	МАКСИМУМ(СчетФактураПолученныйДокументыОснования.Ссылка) КАК СчетФактура,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.Дата) КАК Дата,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.Контрагент) КАК Контрагент,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.КурсВзаиморасчетов) КАК КурсВзаиморасчетов,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.КратностьВзаиморасчетов) КАК КратностьВзаиморасчетов,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.ВалютаДокумента) КАК ВалютаДокумента,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.СуммаВключаетНДС) КАК СуммаВключаетНДС,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.Организация) КАК Организация,
	               |	МАКСИМУМ(ПоступлениеТоваровУслуг.ДоговорКонтрагента) КАК ДоговорКонтрагента
	               |ПОМЕСТИТЬ ВТ_ПоступлениеТоваровУслуг
	               |ИЗ
	               |	(ВЫБРАТЬ
	               |		ПоступлениеТоваровУслуг.Ссылка КАК Ссылка,
	               |		ПоступлениеТоваровУслуг.Дата КАК Дата,
	               |		ПоступлениеТоваровУслуг.Контрагент КАК Контрагент,
	               |		ПоступлениеТоваровУслуг.КурсВзаиморасчетов КАК КурсВзаиморасчетов,
	               |		ПоступлениеТоваровУслуг.КратностьВзаиморасчетов КАК КратностьВзаиморасчетов,
	               |		ПоступлениеТоваровУслуг.ВалютаДокумента КАК ВалютаДокумента,
	               |		ПоступлениеТоваровУслуг.СуммаВключаетНДС КАК СуммаВключаетНДС,
	               |		ПоступлениеТоваровУслуг.Организация КАК Организация,
	               |		ПоступлениеТоваровУслуг.ДоговорКонтрагента КАК ДоговорКонтрагента
	               |	ИЗ
	               |		Документ.ПоступлениеТоваровУслуг КАК ПоступлениеТоваровУслуг
	               |	ГДЕ
	               |		ПоступлениеТоваровУслуг.Дата >= &НачДата
	               |		И ПоступлениеТоваровУслуг.Дата <= &КонДата
	               |		И ПоступлениеТоваровУслуг.ВидОперации <> &ВидОперацииИмпорт
	               |		И ПоступлениеТоваровУслуг.Проведен = ИСТИНА
	               |		И ПоступлениеТоваровУслуг.Организация = &Организация
	               |		И ПоступлениеТоваровУслуг.ВидОперации <> &ВидОперацииСтруктурные) КАК ПоступлениеТоваровУслуг
	               |		ЛЕВОЕ СОЕДИНЕНИЕ Документ.СчетФактураПолученный.ДокументыОснования КАК СчетФактураПолученныйДокументыОснования
	               |		ПО (ПоступлениеТоваровУслуг.Ссылка = (ВЫРАЗИТЬ(СчетФактураПолученныйДокументыОснования.ДокументОснование КАК Документ.ПоступлениеТоваровУслуг)))
	               |
	               |СГРУППИРОВАТЬ ПО
	               |	ПоступлениеТоваровУслуг.Ссылка
	               |
	               |ИНДЕКСИРОВАТЬ ПО
	               |	Ссылка
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ РАЗЛИЧНЫЕ
	               |	НДСЗаНерезидентаОбороты.ДокументПоступления КАК ДокументПоступления,
	               |	НДСЗаНерезидентаОбороты.Номенклатура КАК Номенклатура
	               |ПОМЕСТИТЬ ВТ_НДСЗаНерезидента
	               |ИЗ
	               |	РегистрНакопления.НДСЗаНерезидента.Обороты(
	               |			,
	               |			,
	               |			,
	               |			Налогоплательщик = &Организация
	               |				И ДокументПоступления В
	               |					(ВЫБРАТЬ
	               |						ВТ_ПоступлениеТоваровУслуг.Ссылка
	               |					ИЗ
	               |						ВТ_ПоступлениеТоваровУслуг
	               |			
	               |					ОБЪЕДИНИТЬ ВСЕ
	               |			
	               |					ВЫБРАТЬ
	               |						ВТ_ПоступлениеНМА.Ссылка
	               |					ИЗ
	               |						ВТ_ПоступлениеНМА)) КАК НДСЗаНерезидентаОбороты
	               |
	               |ИНДЕКСИРОВАТЬ ПО
	               |	ДокументПоступления,
	               |	Номенклатура
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	ВложенныйЗапрос.СсылкаНаДокумент,
	               |	ВложенныйЗапрос.ДоговорКонтрагента,
	               |	ВложенныйЗапрос.Контрагент,
	               |	ВложенныйЗапрос.Организация,
	               |	ВложенныйЗапрос.Номенклатура,
	               |	ВложенныйЗапрос.Цена,
	               |	ВложенныйЗапрос.Количество,
	               |	ВложенныйЗапрос.СтавкаНДС,
	               |	ВложенныйЗапрос.СуммаНДС,
	               |	ВложенныйЗапрос.Счет,
	               |	ВложенныйЗапрос.Дата,
	               |	ВложенныйЗапрос.ОснованиеВозврата,
	               |	ВложенныйЗапрос.Сумма,
	               |	ВложенныйЗапрос.СуммаИПН,
	               |	ЕСТЬNULL(ВложенныйЗапрос.ПинКод, моп_ОбъектыМониторинга.ПинКод) КАК ПинКод,
	               |	моп_ОбъектыМониторинга.КодТНВЭД КАК КодТНВЭД,
	               |	ВложенныйЗапрос.ЕдиницаИзмерения,
	               |	ВложенныйЗапрос.ВалютаДокумента,
	               |	ЕСТЬNULL(ВложенныйЗапрос.КратностьВзаиморасчетов, 0) КАК КратностьВзаиморасчетов,
	               |	ЕСТЬNULL(ВложенныйЗапрос.КурсВзаиморасчетов, 0) КАК КурсВзаиморасчетов,
	               |	ВложенныйЗапрос.СчетФактура,
	               |	ВЫБОР
	               |		КОГДА (НЕ ВложенныйЗапрос.Контрагент.Ссылка ЕСТЬ NULL )
	               |				И ВложенныйЗапрос.Контрагент.Ссылка <> ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
	               |				И (ВложенныйЗапрос.Контрагент.ЮрФизЛицо = ЗНАЧЕНИЕ(Перечисление.ЮрФизЛицо.ФизЛицо)
	               |					И (НЕ ВложенныйЗапрос.Контрагент.ИндивидуальныйПредпринимательАдвокатЧастныйНотариус))
	               |			ТОГДА ИСТИНА
	               |		КОГДА ВложенныйЗапрос.Контрагент.Ссылка ЕСТЬ NULL 
	               |				ИЛИ ВложенныйЗапрос.Контрагент.Ссылка = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
	               |			ТОГДА ИСТИНА
	               |		ИНАЧЕ ЛОЖЬ
	               |	КОНЕЦ КАК КонтрагентЯвляетсяЧастнымЛицом
	               |ИЗ
	               |	(ВЫБРАТЬ
	               |		ПоступлениеТоваровУслугТовары.Ссылка КАК СсылкаНаДокумент,
	               |		ВТ_ПоступлениеТоваровУслуг.ДоговорКонтрагента КАК ДоговорКонтрагента,
	               |		ВТ_ПоступлениеТоваровУслуг.Контрагент КАК Контрагент,
	               |		ВТ_ПоступлениеТоваровУслуг.Организация КАК Организация,
	               |		ПоступлениеТоваровУслугТовары.Номенклатура КАК Номенклатура,
	               |		ПоступлениеТоваровУслугТовары.Цена КАК Цена,
	               |		ПоступлениеТоваровУслугТовары.Количество КАК Количество,
	               |		ПоступлениеТоваровУслугТовары.СтавкаНДС КАК СтавкаНДС,
	               |		ПоступлениеТоваровУслугТовары.СуммаНДС КАК СуммаНДС,
	               |		ПоступлениеТоваровУслугТовары.СчетУчетаБУ КАК Счет,
	               |		ВТ_ПоступлениеТоваровУслуг.Дата КАК Дата,
	               |		0 КАК ОснованиеВозврата,
