	ВложенныйЗапрос.Счет,
	ВложенныйЗапрос.Регистратор КАК Регистратор,
	ВложенныйЗапрос.Оборот,
	ВложенныйЗапрос.НачальныйОстаток,
	ВложенныйЗапрос.КонечныйОстаток
ИЗ
	(ВЫБРАТЬ
		Данные100_08_001.НазваниеОбласти КАК НазваниеОбласти,
		Данные100_08_001.Регистратор1 КАК Регистратор,
		Данные100_08_001.Счет КАК Счет,
		ВЫБОР
			КОГДА Данные100_08_001.СуммаОборот = NULL
				ТОГДА 0
			ИНАЧЕ Данные100_08_001.СуммаОборот
		КОНЕЦ КАК Оборот,
		NULL КАК НачальныйОстаток,
		NULL КАК КонечныйОстаток
	ИЗ
		(ВЫБРАТЬ
			ВЫБОР
				КОГДА ТиповойОбороты.Счет В ИЕРАРХИИ (&amp;СписокСчетовА)
					ТОГДА "100.08.002.A"
				КОГДА ТиповойОбороты.Счет В ИЕРАРХИИ (&amp;СписокСчетовB)
					ТОГДА "100.08.002.B"
				ИНАЧЕ "100.08.002.C"
			КОНЕЦ КАК НазваниеОбласти,
			ТиповойОбороты.Регистратор КАК Регистратор1,
			ТиповойОбороты.Счет КАК Счет,
			ТиповойОбороты.СуммаОборот КАК СуммаОборот
		ИЗ
			РегистрБухгалтерии.Типовой.Обороты(&amp;НачПериода, &amp;КонПериода, Регистратор, Счет В ИЕРАРХИИ (&amp;СписокСчетов), , Организация В (&amp;СписокОрганизаций), , ) КАК ТиповойОбороты) КАК Данные100_08_001
	ГДЕ
		Данные100_08_001.НазваниеОбласти В(&amp;НазваниеОбласти)
	
	СГРУППИРОВАТЬ ПО
		Данные100_08_001.НазваниеОбласти,
		Данные100_08_001.Счет,
		Данные100_08_001.Регистратор1,
		ВЫБОР
			КОГДА Данные100_08_001.СуммаОборот = NULL
				ТОГДА 0
			ИНАЧЕ Данные100_08_001.СуммаОборот
		КОНЕЦ
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		Данные100_08_001.НазваниеОбласти,
		NULL,
		Данные100_08_001.Счет,
		NULL,
		ВЫБОР
			КОГДА Данные100_08_001.СуммаОстаток = NULL
				ТОГДА 0
			ИНАЧЕ Данные100_08_001.СуммаОстаток
		КОНЕЦ,
		NULL
	ИЗ
		(ВЫБРАТЬ
			ВЫБОР
				КОГДА ТиповойОстатки.Счет В ИЕРАРХИИ (&amp;СписокСчетовА)
					ТОГДА "100.08.002.A"
				КОГДА ТиповойОстатки.Счет В ИЕРАРХИИ (&amp;СписокСчетовB)
					ТОГДА "100.08.002.B"
				ИНАЧЕ "100.08.002.C"
			КОНЕЦ КАК НазваниеОбласти,
			ТиповойОстатки.СуммаОстаток КАК СуммаОстаток,
			ТиповойОстатки.Счет КАК Счет
		ИЗ
			РегистрБухгалтерии.Типовой.Остатки(&amp;НачПериода, Счет В ИЕРАРХИИ (&amp;СписокСчетов), , Организация В (&amp;СписокОрганизаций)) КАК ТиповойОстатки) КАК Данные100_08_001
	ГДЕ
		Данные100_08_001.НазваниеОбласти В(&amp;НазваниеОбласти)
	
	СГРУППИРОВАТЬ ПО
		Данные100_08_001.НазваниеОбласти,
		Данные100_08_001.Счет,
		ВЫБОР
			КОГДА Данные100_08_001.СуммаОстаток = NULL
				ТОГДА 0
			ИНАЧЕ Данные100_08_001.СуммаОстаток
		КОНЕЦ
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		Данные100_08_001.НазваниеОбласти,
		NULL,
		Данные100_08_001.Счет,
		NULL,
		NULL,
		ВЫБОР
			КОГДА Данные100_08_001.СуммаОстаток = NULL
				ТОГДА 0
			ИНАЧЕ Данные100_08_001.СуммаОстаток
		КОНЕЦ
	ИЗ
		(ВЫБРАТЬ
			ВЫБОР
				КОГДА ТиповойОстатки.Счет В ИЕРАРХИИ (&amp;СписокСчетовА)
					ТОГДА "100.08.002.A"
				КОГДА ТиповойОстатки.Счет В ИЕРАРХИИ (&amp;СписокСчетовB)
					ТОГДА "100.08.002.B"
				ИНАЧЕ "100.08.002.C"
			КОНЕЦ КАК НазваниеОбласти,
			ТиповойОстатки.СуммаОстаток КАК СуммаОстаток,
			ТиповойОстатки.Счет КАК Счет
		ИЗ
			РегистрБухгалтерии.Типовой.Остатки(&amp;КонПериода, Счет В ИЕРАРХИИ (&amp;СписокСчетов), , Организация В (&amp;СписокОрганизаций)) КАК ТиповойОстатки) КАК Данные100_08_001
	ГДЕ
		Данные100_08_001.НазваниеОбласти В(&amp;НазваниеОбласти)
	
	СГРУППИРОВАТЬ ПО
		Данные100_08_001.НазваниеОбласти,
		Данные100_08_001.Счет,
		ВЫБОР
			КОГДА Данные100_08_001.СуммаОстаток = NULL
				ТОГДА 0
			ИНАЧЕ Данные100_08_001.СуммаОстаток
		КОНЕЦ) КАК ВложенныйЗапрос</query>
