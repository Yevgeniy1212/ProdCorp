<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Регистратор</dataPath>
			<field>Регистратор</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Ссылка</dataPath>
			<field>Ссылка</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВозвратДеньгами</dataPath>
			<field>ВозвратДеньгами</field>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВозвратЗерном</dataPath>
			<field>ВозвратЗерном</field>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Договор</dataPath>
			<field>Договор</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДоговорВладелец</dataPath>
			<field>ДоговорВладелец</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДоговорКонтрагента</dataPath>
			<field>ДоговорКонтрагента</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЗаявленнаяПосевнаяПлощадь</dataPath>
			<field>ЗаявленнаяПосевнаяПлощадь</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЗаявленнаяПосевнаяПлощадь1</dataPath>
			<field>ЗаявленнаяПосевнаяПлощадь1</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЗаявленнаяПосевнаяПлощадь2</dataPath>
			<field>ЗаявленнаяПосевнаяПлощадь2</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоЗаявок</dataPath>
			<field>КоличествоЗаявок</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоЗаявок1</dataPath>
			<field>КоличествоЗаявок1</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоЗаявок2</dataPath>
			<field>КоличествоЗаявок2</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоОплаченныхЗаявок</dataPath>
			<field>КоличествоОплаченныхЗаявок</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоОплаченныхСХТП</dataPath>
			<field>КоличествоОплаченныхСХТП</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСХТП</dataPath>
			<field>КоличествоСХТП</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСХТП1</dataPath>
			<field>КоличествоСХТП1</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСХТП2</dataPath>
			<field>КоличествоСХТП2</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Контрагент</dataPath>
			<field>Контрагент</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Организация</dataPath>
			<field>Организация</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Поставщик</dataPath>
			<field>Поставщик</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Район</dataPath>
			<field>Район</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаОборотКт</dataPath>
			<field>СуммаОборотКт</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаОборотДт</dataPath>
			<field>СуммаОборотДт</field>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаДоговора</dataPath>
			<field>СуммаДоговора</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаДоговора1</dataPath>
			<field>СуммаДоговора1</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаДоговора2</dataPath>
			<field>СуммаДоговора2</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаДокумента</dataPath>
			<field>СуммаДокумента</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаКомиссии</dataPath>
			<field>СуммаКомиссии</field>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Остаток</dataPath>
			<field>Остаток</field>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	цс_Договор.Ссылка КАК Ссылка,
	цс_Договор.ДоговорКонтрагента КАК ДоговорКонтрагента,
	цс_Договор.Поставщик КАК Поставщик,
	цс_Договор.СуммаДокумента КАК СуммаДокумента,
	цс_Договор.СуммаКомиссии КАК СуммаКомиссии,
	цс_Договор.Организация КАК Организация,
	цс_Договор.ДокументОснование.ПосевнаяПлощадь КАК ЗаявленнаяПосевнаяПлощадь
ПОМЕСТИТЬ ДоговорыФЗ
{ВЫБРАТЬ
	Ссылка.*,
	ДоговорКонтрагента.*,
	Поставщик.*,
	СуммаДокумента,
	СуммаКомиссии,
	Организация.*,
	ЗаявленнаяПосевнаяПлощадь}
ИЗ
	Документ.цс_Договор КАК цс_Договор
ГДЕ
	цс_Договор.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И НЕ цс_Договор.Ссылка В
				(ВЫБРАТЬ
					ЦС_РасторгнутыеДоговораФорвардногоЗакупа.ЦС_Договор
				ИЗ
					РегистрСведений.ЦС_РасторгнутыеДоговораФорвардногоЗакупа КАК ЦС_РасторгнутыеДоговораФорвардногоЗакупа)
{ГДЕ
	цс_Договор.Ссылка.*,
	цс_Договор.ДоговорКонтрагента.*,
	цс_Договор.Поставщик.*,
	цс_Договор.СуммаДокумента,
	цс_Договор.СуммаКомиссии,
	цс_Договор.Организация.*,
	цс_Договор.ДокументОснование.ПосевнаяПлощадь КАК ЗаявленнаяПосевнаяПлощадь}

ИНДЕКСИРОВАТЬ ПО
	Ссылка,
	ДоговорКонтрагента,
	Поставщик,
	Организация
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	КонтактнаяИнформация.Объект КАК Контрагент,
	КонтактнаяИнформация.Поле3 КАК Район
ПОМЕСТИТЬ КИ
{ВЫБРАТЬ
	Контрагент.*,
	Район}
ИЗ
	РегистрСведений.КонтактнаяИнформация КАК КонтактнаяИнформация
ГДЕ
	КонтактнаяИнформация.Тип = &amp;ТипАдрес
	И КонтактнаяИнформация.Вид = &amp;ВидЮрАдрес
{ГДЕ
	КонтактнаяИнформация.Объект.* КАК Контрагент,
	КонтактнаяИнформация.Поле3 КАК Район}

ИНДЕКСИРОВАТЬ ПО
	Контрагент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТиповойОбороты.Субконто1 КАК Договор,
	СУММА(ТиповойОбороты.СуммаОборотДт) КАК СуммаОборотДт
ПОМЕСТИТЬ Оплаты
{ВЫБРАТЬ
	Договор.*,
	СуммаОборотДт}
ИЗ
	РегистрБухгалтерии.Типовой.Обороты(
			,
			,
			Регистратор,
			,
			&amp;СубконтоДоговоры,
			субконто1 В
				(ВЫБРАТЬ
					ДоговорыФЗ.ДоговорКонтрагента
				ИЗ
					ДоговорыФЗ КАК ДоговорыФЗ),
			,
			) КАК ТиповойОбороты
ГДЕ
	ТИПЗНАЧЕНИЯ(ТиповойОбороты.Регистратор) = ТИП(документ.платежноепоручениеисходящее)
{ГДЕ
	ТиповойОбороты.Субконто1.* КАК Договор,
	ТиповойОбороты.СуммаОборотДт}

СГРУППИРОВАТЬ ПО
	ТиповойОбороты.Субконто1

ИНДЕКСИРОВАТЬ ПО
	Договор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТиповойОбороты.Субконто1 КАК Договор,
	ТиповойОбороты.Регистратор КАК Регистратор,
	ТиповойОбороты.СуммаОборотКт КАК СуммаОборот
ПОМЕСТИТЬ Возвраты
{ВЫБРАТЬ
	Договор.*,
	Регистратор.*,
	СуммаОборот}
ИЗ
	РегистрБухгалтерии.Типовой.Обороты(
			&amp;НачалоПериода,
			&amp;КонецПериода,
			Регистратор,
			,
			&amp;СубконтоДоговоры,
			Субконто1 В
				(ВЫБРАТЬ
					ДоговорыФЗ.ДоговорКонтрагента
				ИЗ
					ДоговорыФЗ КАК ДоговорыФЗ),
			,
			) КАК ТиповойОбороты
{ГДЕ
	ТиповойОбороты.Субконто1.* КАК Договор,
	ТиповойОбороты.Регистратор.*,
	ТиповойОбороты.СуммаОборотКт}

ИНДЕКСИРОВАТЬ ПО
	Договор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Возвраты.Договор КАК Договор,
	СУММА(ВЫБОР
			КОГДА ТИПЗНАЧЕНИЯ(Возвраты.Регистратор) = ТИП(документ.платежноепоручениевходящее)
				ТОГДА Возвраты.СуммаОборот
			ИНАЧЕ 0
		КОНЕЦ) КАК ВозвратДеньгами,
	СУММА(ВЫБОР
			КОГДА ТИПЗНАЧЕНИЯ(Возвраты.Регистратор) = ТИП(документ.ур_поступлениезерна)
				ТОГДА Возвраты.СуммаОборот
			ИНАЧЕ 0
		КОНЕЦ) КАК ВозвратЗерном
ПОМЕСТИТЬ ВозвратыСгрупп
{ВЫБРАТЬ
	Договор.*,
	ВозвратДеньгами,
	ВозвратЗерном}
ИЗ
	Возвраты КАК Возвраты
{ГДЕ
	Возвраты.Договор.*,
	(ВЫБОР
			КОГДА ТИПЗНАЧЕНИЯ(Возвраты.Регистратор) = ТИП(документ.платежноепоручениевходящее)
				ТОГДА Возвраты.СуммаОборот
			ИНАЧЕ 0
		КОНЕЦ) КАК ВозвратДеньгами,
	(ВЫБОР
			КОГДА ТИПЗНАЧЕНИЯ(Возвраты.Регистратор) = ТИП(документ.ур_поступлениезерна)
				ТОГДА Возвраты.СуммаОборот
			ИНАЧЕ 0
		КОНЕЦ) КАК ВозвратЗерном}

СГРУППИРОВАТЬ ПО
	Возвраты.Договор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДоговорыФЗ.Организация КАК Организация,
	ДоговорыФЗ.Поставщик КАК Поставщик,
	КИ.Район КАК Район,
	СУММА(ЕСТЬNULL(Оплаты.СуммаОборотДт, 0)) КАК СуммаОборотДт,
	СУММА(ДоговорыФЗ.СуммаДокумента) КАК СуммаДоговора,
	КОЛИЧЕСТВО(Оплаты.Договор) КАК КоличествоОплаченныхЗаявок,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ Оплаты.Договор.Владелец) КАК КоличествоОплаченныхСХТП,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ДоговорыФЗ.Поставщик) КАК КоличествоСХТП,
	КОЛИЧЕСТВО(ДоговорыФЗ.ДоговорКонтрагента) КАК КоличествоЗаявок,
	СУММА(ДоговорыФЗ.ЗаявленнаяПосевнаяПлощадь) КАК ЗаявленнаяПосевнаяПлощадь,
	СУММА(ЕСТЬNULL(ВозвратыСгрупп.ВозвратДеньгами, 0)) КАК ВозвратДеньгами,
	СУММА(ЕСТЬNULL(ВозвратыСгрупп.ВозвратЗерном, 0)) КАК ВозвратЗерном,
	СУММА(ДоговорыФЗ.СуммаКомиссии) КАК СуммаКомиссии
ПОМЕСТИТЬ ВТ
{ВЫБРАТЬ
	Организация.*,
	Поставщик.*,
	Район,
	СуммаОборотДт,
	СуммаДоговора,
	КоличествоОплаченныхЗаявок.*,
	КоличествоОплаченныхСХТП.*,
	КоличествоСХТП.*,
	КоличествоЗаявок.*,
	ЗаявленнаяПосевнаяПлощадь,
	ВозвратДеньгами,
	ВозвратЗерном,
	СуммаКомиссии}
ИЗ
	ДоговорыФЗ КАК ДоговорыФЗ
		ЛЕВОЕ СОЕДИНЕНИЕ КИ КАК КИ
		ПО ДоговорыФЗ.Поставщик = КИ.Контрагент
		ЛЕВОЕ СОЕДИНЕНИЕ Оплаты КАК Оплаты
		ПО ДоговорыФЗ.ДоговорКонтрагента = Оплаты.Договор
		ЛЕВОЕ СОЕДИНЕНИЕ ВозвратыСгрупп КАК ВозвратыСгрупп
		ПО ДоговорыФЗ.ДоговорКонтрагента = ВозвратыСгрупп.Договор
{ГДЕ
	ДоговорыФЗ.Организация.*,
	ДоговорыФЗ.Поставщик.*,
	КИ.Район,
	(ЕСТЬNULL(Оплаты.СуммаОборотДт, 0)) КАК СуммаОборотДт,
	ДоговорыФЗ.СуммаДокумента,
	Оплаты.Договор.*,
	Оплаты.Договор.Владелец.*,
	ДоговорыФЗ.ДоговорКонтрагента.*,
	ДоговорыФЗ.ЗаявленнаяПосевнаяПлощадь,
	(ЕСТЬNULL(ВозвратыСгрупп.ВозвратДеньгами, 0)) КАК ВозвратДеньгами,
	(ЕСТЬNULL(ВозвратыСгрупп.ВозвратЗерном, 0)) КАК ВозвратЗерном,
	ДоговорыФЗ.СуммаКомиссии}

СГРУППИРОВАТЬ ПО
	ДоговорыФЗ.Поставщик,
	ДоговорыФЗ.Организация,
	КИ.Район

ИНДЕКСИРОВАТЬ ПО
	Организация,
	Поставщик
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ.Организация КАК Организация,
	ВТ.Поставщик КАК Поставщик,
	ВТ.Район КАК Район,
	ВТ.СуммаОборотДт КАК СуммаОборотДт,
	ВТ.СуммаДоговора КАК СуммаДоговора,
	ВТ.КоличествоОплаченныхЗаявок КАК КоличествоОплаченныхЗаявок,
	ВТ.КоличествоОплаченныхСХТП КАК КоличествоОплаченныхСХТП,
	ВТ.КоличествоСХТП КАК КоличествоСХТП,
	ВТ.КоличествоЗаявок КАК КоличествоЗаявок,
	ВТ.ЗаявленнаяПосевнаяПлощадь КАК ЗаявленнаяПосевнаяПлощадь,
	ВТ.СуммаДоговора КАК СуммаДоговора1,
	ВТ.КоличествоСХТП КАК КоличествоСХТП1,
	ВТ.КоличествоЗаявок КАК КоличествоЗаявок1,
	ВТ.ЗаявленнаяПосевнаяПлощадь КАК ЗаявленнаяПосевнаяПлощадь1,
	ВТ.СуммаДоговора КАК СуммаДоговора2,
	ВТ.КоличествоСХТП КАК КоличествоСХТП2,
	ВТ.КоличествоЗаявок КАК КоличествоЗаявок2,
	ВТ.ЗаявленнаяПосевнаяПлощадь КАК ЗаявленнаяПосевнаяПлощадь2,
	ВТ.ВозвратДеньгами КАК ВозвратДеньгами,
	ВТ.ВозвратЗерном КАК ВозвратЗерном,
	ВТ.СуммаКомиссии КАК СуммаКомиссии,
	ВТ.СуммаКомиссии + ВТ.СуммаОборотДт - ВТ.ВозвратДеньгами - ВТ.ВозвратЗерном КАК Остаток
{ВЫБРАТЬ
	Организация.*,
	Поставщик.*,
	Район,
	СуммаОборотДт,
	СуммаДоговора,
	КоличествоОплаченныхЗаявок,
	КоличествоОплаченныхСХТП,
	КоличествоСХТП,
	КоличествоЗаявок,
	ЗаявленнаяПосевнаяПлощадь,
	СуммаДоговора1,
	КоличествоСХТП1,
	КоличествоЗаявок1,
	ЗаявленнаяПосевнаяПлощадь1,
	СуммаДоговора2,
	КоличествоСХТП2,
	КоличествоЗаявок2,
	ЗаявленнаяПосевнаяПлощадь2,
	ВозвратДеньгами,
	ВозвратЗерном,
	СуммаКомиссии,
	Остаток}
ИЗ
	ВТ КАК ВТ
{ГДЕ
	ВТ.Организация.*,
	ВТ.Поставщик.*,
	ВТ.Район,
	ВТ.СуммаОборотДт,
	ВТ.СуммаДоговора,
	ВТ.КоличествоОплаченныхЗаявок,
	ВТ.КоличествоОплаченныхСХТП,
	ВТ.КоличествоСХТП,
	ВТ.КоличествоЗаявок,
	ВТ.ЗаявленнаяПосевнаяПлощадь,
	ВТ.ВозвратДеньгами,
	ВТ.ВозвратЗерном,
	ВТ.СуммаКомиссии,
	(ВТ.СуммаКомиссии + ВТ.СуммаОборотДт - ВТ.ВозвратДеньгами - ВТ.ВозвратЗерном) КАК Остаток}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
