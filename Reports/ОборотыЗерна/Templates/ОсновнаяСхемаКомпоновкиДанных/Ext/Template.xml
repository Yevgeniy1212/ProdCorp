	ВложенныйЗапрос.КоличествоПеремПриход,
	ВложенныйЗапрос.СуммаПеремПриход,
	ВложенныйЗапрос.КоличествоПеремРасход,
	ВложенныйЗапрос.СуммаПеремРасход,
	ЕСТЬNULL(Остатки.Счет, ВложенныйЗапрос.Счет) КАК Счет,
	Остатки.СуммаНачальныйОстаток КАК СуммаНачальныйОстаток,
	Остатки.СуммаКонечныйОстаток КАК СуммаКонечныйОстаток,
	Остатки.КоличествоНачальныйОстаток,
	Остатки.КоличествоКонечныйОстаток,
	ВЫБОР
		КОГДА ВложенныйЗапрос.КоличествоЗакуп &lt;&gt; 0
			ТОГДА ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаЗакуп / ВложенныйЗапрос.КоличествоЗакуп КАК ЧИСЛО(12, 2))
		ИНАЧЕ ВложенныйЗапрос.СуммаЗакуп
	КОНЕЦ КАК ЦенаЗакуп,
	ВЫБОР
		КОГДА ВложенныйЗапрос.КоличествоРасходМемо &lt;&gt; 0
			ТОГДА ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаРасходМемо / ВложенныйЗапрос.КоличествоРасходМемо КАК ЧИСЛО(12, 2))
		ИНАЧЕ ВложенныйЗапрос.СуммаРасходМемо
	КОНЕЦ КАК ЦенаРасходМемо,
	ВЫБОР
		КОГДА ВложенныйЗапрос.КоличествоПостБартер &lt;&gt; 0
			ТОГДА ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаПостБартер / ВложенныйЗапрос.КоличествоПостБартер КАК ЧИСЛО(12, 2))
		ИНАЧЕ ВложенныйЗапрос.СуммаПостБартер
	КОНЕЦ КАК ЦенаПостБартер,
	ВЫБОР
		КОГДА ВложенныйЗапрос.КоличествоРасходЭкспорт &lt;&gt; 0
			ТОГДА ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаРасходЭкспорт / ВложенныйЗапрос.КоличествоРасходЭкспорт КАК ЧИСЛО(12, 2))
		ИНАЧЕ ВложенныйЗапрос.СуммаРасходЭкспорт
	КОНЕЦ КАК ЦенаРасходЭкспорт,
	ВЫБОР
		КОГДА ВложенныйЗапрос.КоличествоРасходБартер &lt;&gt; 0
			ТОГДА ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаРасходБартер / ВложенныйЗапрос.КоличествоРасходБартер КАК ЧИСЛО(12, 2))
		ИНАЧЕ ВложенныйЗапрос.СуммаРасходБартер
	КОНЕЦ КАК ЦенаРасходБартер,
	ВЫБОР
		КОГДА ВложенныйЗапрос.КоличествоРасходРеализация &lt;&gt; 0
