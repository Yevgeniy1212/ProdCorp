ИЗ
	РегистрНакопления.фин_ОборотыБюджетов КАК Документ
ГДЕ
	Документ.Период &gt;= &amp;НачалоПериода
	И (Документ.Период &lt;= &amp;КонецПериода
			ИЛИ &amp;КонецПериода = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0))</query>
	</dataSet>
	<dataSetLink>
		<sourceDataSet>Основной</sourceDataSet>
		<destinationDataSet>Связанный</destinationDataSet>
		<sourceExpression>Документ</sourceExpression>
		<destinationExpression>Документ</destinationExpression>
